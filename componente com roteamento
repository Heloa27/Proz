  return (
    <Router>
      <div>
        <nav>
          {/* Links para navegar entre as telas */}
          <ul>
            <li>
              <Link to="/tela1">Tela 1</Link>
            </li>
            <li>
              <Link to="/tela2">Tela 2</Link>
            </li>
          </ul>
        </nav>

        {/* Definição das rotas para cada tela */}
        <Route path="/tela1" component={Tela1} />
        <Route
